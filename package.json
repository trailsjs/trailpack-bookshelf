{
  "name": "trailpack-bookshelf",
  "version": "1.0.0-alpha-1",
  "description": "Bookshelf Trailpack",
  "homepage": "https://github.com/trailsjs/trailpack-bookshelf",
  "author": {
    "name": "Leonid Kuzmin",
    "email": "lndkuzmin@gmail.com",
    "url": "http://zuker.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trailsjs/trailpack-bookshelf.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "trailpack",
    "trails",
    "trailjs"
  ],
  "dependencies": {
    "bluebird": "^3.4.0",
    "bookshelf": "^0.9.5",
    "bookshelf-schema": "^0.2.3",
    "joi": "^8.1.0",
    "knex": "^0.11.3",
    "lodash": "^4.13.1",
    "trailpack-datastore": "^1.0.1",
    "trails-service": "^1.0.0-beta-2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codecov": "^1.0.1",
    "eslint": "^2.10.2",
    "eslint-config-trails": "^1.0.6",
    "mocha": "^2.5.0",
    "nyc": "^6.4.4",
    "smokesignals": "^1.2.2",
    "sqlite3": "^3.1.4",
    "trailpack-core": "^1.0.0-beta-6",
    "trails": "^1.0.0",
    "trails-model": "^1.0.0-beta-2"
  },
  "scripts": {
    "clean:coverage": "rm -rf ./coverage",
    "test": "node_modules/.bin/eslint src && node_modules/.bin/mocha",
    "coverage": "npm run clean:coverage && node_modules/.bin/nyc --require babel-register --reporter lcovonly npm test && ./node_modules/.bin/codecov",
    "clean": "rm -rf ./dist && rm -f ./test/.app.db && npm run clean:coverage",
    "prepublish": "npm run clean && node_modules/.bin/babel --source-maps -d ./dist ./src"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "license": "MIT",
  "nyc": {
    "exclude": [
      ".eslintrc.js",
      "test"
    ]
  }
}
